{
  "compilerOptions": {
    "target": "es5",                          
    "module": "commonjs",                    
    "lib": ["es6", "dom"],                     
    "allowJs": true,
    "outDir": "build",                          
    "rootDir": "src",
    "strict": false,         
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": ["dashboard", "node_modules"]
}